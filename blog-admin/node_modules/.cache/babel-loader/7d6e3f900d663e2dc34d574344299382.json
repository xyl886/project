photo-src-list=\"photoList\"\n                />\n                <div class=\"photo-name\">{{ item.name }}</div>\n              </div>\n            </el-checkbox>\n          </el-col>\n        </el-checkbox-group>\n      </el-row>\n      <!-- 分页 -->\n      <el-pagination\n        :hide-on-single-page=\"true\"\n        class=\"pagination-container\"\n        :current-page=\"params.pageNo\"\n        :page-size=\"params.pageSize\"\n        :total=\"total\"\n        layout=\"prev, pager, next\"\n        @size-change=\"sizeChange\"\n        @current-change=\"currentChange\"\n      />\n\n      <!-- 新增编辑对话框 -->\n      <el-dialog center :title=\"title\" :visible.sync=\"dialogVisible\" width=\"35%\">\n        <el-form ref=\"dataForm\" :rules=\"rules\" label-width=\"80px\" size=\"medium\" :model=\"photoForm\">\n          <el-form-item label=\"照片名称\" prop=\"name\">\n            <el-input v-model=\"photoForm.name\" style=\"width:220px\" />\n          </el-form-item>\n          <el-form-item label=\"照片描述\" prop=\"info\">\n            <el-input v-model=\"photoForm.info\" style=\"width:220px\" />\n          </el-form-item>\n          <el-form-item label=\"照片\" prop=\"url\">\n            <el-upload\n              v-loading=\"imgLoading\"\n              class=\"upload-cover\"\n              drag\n              :show-file-list=\"false\"\n              multiple\n              :action=\"uploadPictureHost\"\n              :before-upload=\"beforeUpload\"\n              :http-request=\"uploadSectionFile\"\n            >\n              <i v-if=\"photoForm.url === ''\" class=\"el-icon-upload\" />\n              <div v-if=\"photoForm.url === ''\" class=\"el-upload__text\">\n                将文件拖到此处，或<em>点击上传</em>\n              </div>\n              <img\n                v-else\n                :src=\"photoForm.url\"\n                width=\"360px\"\n                height=\"180px\"\n              >\n            </el-upload>\n          </el-form-item>\n        </el-form>\n        <div slot=\"footer\">\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\n          <el-button type=\"primary\" @click=\"submit\">确定</el-button>\n        </div>\n      </el-dialog>\n\n      <!-- 移动对话框 -->\n      <el-dialog :visible.sync=\"movePhoto\" width=\"30%\">\n        <div slot=\"title\" class=\"dialog-title-container\">\n          移动照片\n        </div>\n        <el-empty v-if=\"albumList.length < 2\" description=\"暂无其他相册\" />\n        <el-form v-else label-width=\"80px\" size=\"medium\" :model=\"photoForm\">\n          <el-radio-group v-model=\"albumId\">\n            <div class=\"album-check-item\">\n              <template v-for=\"item of albumList\">\n                <el-radio\n                  v-if=\"item.id !== albumInfo.id\"\n                  :key=\"item.id\"\n                  :label=\"item.id\"\n                  style=\"margin-bottom:1rem\"\n                >\n                  <div class=\"album-check\">\n                    <el-image\n                      fit=\"cover\"\n                      class=\"album-check-cover\"\n                      :src=\"item.cover\"\n                    />\n                    <div style=\"margin-left:0.5rem\">{{ item.name }}</div>\n                  </div>\n                </el-radio>\n              </template>\n            </div>\n          </el-radio-group>\n        </el-form>\n        <div slot=\"footer\">\n          <el-button @click=\"movePhoto = false\">取 消</el-button>\n          <el-button\n            :disabled=\"albumId == null\"\n            type=\"primary\"\n            @click=\"updatePhotoAlbum\"\n          >\n            确 定\n          </el-button>\n        </div>\n      </el-dialog>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { fetchPhoto, deleteBatch, addPhoto, updatePhoto, infoPhoto, movePhoto } from '@/api/photo'\nimport { fetchAlbum, info } from '@/api/photoAlbum'\nimport { upload } from '@/api/imgUpload'\nimport { mapGetters } from 'vuex'\nimport { hasAuth } from '@/utils/aut